<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_municipalities_tree" model="ir.ui.view">
            <field name="name">res.country.municipality.tree</field>
            <field name="model">res.country.municipality</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="municipalities">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="state_id"/>
                    <field name="parish_ids"/>
                </tree>
            </field>
        </record>

        <record id="view_municipalities_form" model="ir.ui.view">
            <field name="name">res.country.municipality.form</field>
            <field name="model">res.country.municipality</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="municipalities">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="state_id"/>
                    <field name="parish_ids"/>
                </form>
            </field>
        </record>

    </data>
</openerp>
