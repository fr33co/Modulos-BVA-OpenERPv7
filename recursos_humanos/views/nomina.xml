<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	
	<data>
			
	    <!--Accionador para el menu de nomina adm-->
	    <record model="ir.actions.act_window" id="accion_nomina_adm">
		<field name="name">Nomina</field>
		<field name="res_model">hr.nomina.adm</field>
		<field name="view_type">form</field>
		<field name="view_mode">tree,form</field>
		<field name="help" type="html">
			<p class="oe_view_nocontent_create">Registrar Nomina</p>
		</field>
	    </record>
	    <!--Accionador para el menu de nomina adm-->
	    
	    <record model="ir.ui.view" id="vista_nomina_adm">
		<field name="name">Nomina.form</field>
		<field name="model">hr.nomina.adm</field>
		<field name="arch" type="xml">
		    <form string="Formulario de nomina">
			<group colspan="2" col="4">
			    <field name="cod" nolabel="0" colspan="2"/>
			    <field name="nomina" nolabel="0" colspan="2"/>
			</group>
		    </form>
		</field>
	    </record>
			
	</data>
	
</openerp>

