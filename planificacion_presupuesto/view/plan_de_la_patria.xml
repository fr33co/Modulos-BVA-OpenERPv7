<!--Form y Tree Plan de Gobierno-->

		<record model="ir.ui.view" id="plan_patria_tree_view">
			<field name="name">Plan de patria</field>
			<field name="model">plan.patria</field>
			<field name="arch" type="xml">
				<tree string="Planes">
					<field name="plan_patria"/>
				</tree>
			</field>			
		</record>
